{
    "name": "mcp-mitre-attack",
    "version": "0.1.0",
    "description": "Multi-agent Coordination Platform (MCP) server for MITRE ATT&CK framework integration",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
        "test": "jest",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
        "prepare": "husky install"
    },
    "keywords": [
        "mitre",
        "attack",
        "security",
        "analysis",
        "llm",
        "azure-openai"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "better-sqlite3": "^8.5.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.0.0",
        "morgan": "^1.10.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.4",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.3",
        "@types/morgan": "^1.9.4",
        "@types/node": "^20.4.5",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "husky": "^8.0.3",
        "jest": "^29.6.2",
        "lint-staged": "^13.2.3",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md}": [
            "prettier --write"
        ]
    }
}